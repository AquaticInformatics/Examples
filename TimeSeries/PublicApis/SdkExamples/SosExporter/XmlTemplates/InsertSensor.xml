<InsertSensor xmlns="http://www.opengis.net/swes/2.0" service="SOS" version="2.0.0">
  <procedureDescriptionFormat>http://www.opengis.net/sensorML/1.0.1</procedureDescriptionFormat>
  <procedureDescription>
    <SensorML xmlns="http://www.opengis.net/sensorML/1.0.1" version="1.0.1">
      <member>
        <System>
          <identification>
            <IdentifierList>
              <identifier name="uniqueID">
                <Term definition="urn:ogc:def:identifier:OGC:1.0:uniqueID">
                  <value>{__procedureUniqueId__}</value>
                </Term>
              </identifier>
            </IdentifierList>
          </identification>
          <capabilities name="featuresOfInterest">
            <ns:DataRecord xmlns:ns="http://www.opengis.net/swe/1.0.1">
              <ns:field name="featureOfInterestID">
                <ns:Text definition="http://www.opengis.net/def/featureOfInterest/identifier">
                  <ns:value>{__featureOfInterestId__}</ns:value>
                </ns:Text>
              </ns:field>
            </ns:DataRecord>
          </capabilities>
        </System>
      </member>
    </SensorML>
  </procedureDescription>
  <observableProperty>{__observablePropertyName__}</observableProperty>
  <metadata>
    <ns:SosInsertionMetadata xmlns:ns="http://www.opengis.net/sos/2.0">
      <ns:observationType>http://www.opengis.net/def/observationType/OGC-OM/2.0/OM_Measurement</ns:observationType>
      <ns:featureOfInterestType>http://www.opengis.net/def/samplingFeatureType/OGC-OM/2.0/SF_SamplingPoint</ns:featureOfInterestType>
    </ns:SosInsertionMetadata>
  </metadata>
</InsertSensor>